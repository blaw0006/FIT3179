{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
 "data": {
    "url": "https://raw.githubusercontent.com/blaw0006/FIT3179/main/assignment2/data/clean_vaccination_data.csv",
    "format": {
      "type": "csv",
      "parse": {
        "Nominalised_vaccinations": "number",
        "Population": "number",
        "Registered": "number"
      }
    }
  },

"transform": [
  {
    "calculate": "datum.Registered / datum.Population",
    "as": "Nominalised_registered"
  }
],

  "mark": "bar",
  "encoding": {
    "x": {
      "field": "State",
      "type": "ordinal",
      "title": "State"
    },
    "y": {
      "field": "Nominalised Vaccinations",
      "aggregate": "count",
      "type": "quantitative"
    },
    "color": {
      "field": "weather",
      "type": "nominal",
      "scale": {
        "domain": ["sun", "fog", "drizzle", "rain", "snow"],
        "range": ["#e7ba52", "#c7c7c7", "#aec7e8", "#1f77b4", "#9467bd"]
      },
      "title": "Weather type"
    }
  }
}